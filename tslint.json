{
	"defaultSeverity": "warn",
	"extends":         [
		"tslint:recommended"
	],
	"jsRules":         {},
	"rules":           {
		"no-console":     false,
		"no-namespace":   false,
		"indent":         [true, "tabs", 2],
		"max-classes-per-file": false,
		"interface-name": false,
		"align": false,
		"interface-over-type-literal": false
	},
	"rulesDirectory":  []
}